@using WebMarket_Web.Areas.Admin.Models.Category
@model EditCategoryViewModel
@{
      ViewData["Title"] = "Admin";
    ViewData["Page"] = "Caregory";
    ViewData["SubPage"] = "Edit";
}
<div class="pagetitle">
 
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="index.html">@ViewData["Title"]</a></li>
            <li class="breadcrumb-item">@ViewData["Page"]</li>
            <li class="breadcrumb-item active">@ViewData["SubPage"]</li>
            <li class="breadcrumb-item active">@Model.Title</li>
        </ol>
    </nav>
</div>
<div class="col-lg-12">

    <div class="card">
        <div class="card-body">
            <h5 class="card-title">Edit @Model.Title</h5>

            <!-- General Form Elements -->
            <form asp-area="Admin" asp-controller="Category" asp-action="Edit" method="post">
                <div class="row mb-3">
                    @Html.EditorFor(m => m.Title)
                </div>
                <div class="row mb-3">
                    @Html.EditorFor(m => m.Slug)
                </div>

                <div class="row mb-3">
                    @Html.EditorFor(m => m.MetaTag)
                </div>

                <div class="row mb-3">
                    @Html.EditorFor(m => m.MetaDescription)
                </div>

                <div class="row mb-3">
                    @Html.EditorFor(m => m.Description)
                </div>

                <div class="col-sm-10">
                    <button type="submit" class="btn btn-primary">Edit</button>
                  </div>

            </form><!-- End General Form Elements -->

        </div>
    </div>

</div>









